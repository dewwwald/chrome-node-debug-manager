{
	"manifest_version": 2,
	"name": "NodeJs Debug Manager",
	"version": "0.0.2",
	"icons": {
		"128": "128.png"
	},
	"background": {
		"scripts": [
			"main.js"
		]
	},
	"permissions": [
		"background",
		"tabs",
		"windows",
		"file:///Users/dewald/node-debugger-url.json"
	],
	"browser_action": {
		"default_popup": "popup/default-popup.html"
	}
}
